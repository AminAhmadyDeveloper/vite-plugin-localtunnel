{
  "name": "@amindevs/vite-plugin-localtunnel",
  "version": "1.0.0",
  "description": "a plugin for vite to start tunnel using localtunnel",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunchee",
    "dev": "bunchee --watch",
    "dev:website": "turbo run dev --filter=website...",
    "dev:test": "turbo run dev --filter=test...",
    "format": "prettier --write .",
    "test": "playwright test"
  },
  "keywords": [
    "vite",
    "plugin",
    "tunnel",
    "localtunnel"
  ],
  "author": {
    "email": "aminAhmadyDeveloper@gmail.com",
    "name": "Amin Ahmady",
    "url": "https://aminahmady.vercel.app"
  },
  "license": "MIT",
  "homepage": "https://github.com/AminAhmadyDeveloper/vite-plugin-localtunnel",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AminAhmadyDeveloper/vite-plugin-localtunnel.git"
  },
  "bugs": {
    "url": "https://github.com/AminAhmadyDeveloper/vite-plugin-localtunnel/issues"
  },
  "packageManager": "npm@10.2.4",
  "devDependencies": {
    "@types/localtunnel": "^2.0.4",
    "@types/node": "^20.11.0",
    "bunchee": "^4.3.3",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.3.5"
  },
  "peerDependencies": {
    "vite": "^2.9.0 || ^3.0.0 || ^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "localtunnel": "^2.0.2",
    "picocolors": "^1.0.1"
  }
}
